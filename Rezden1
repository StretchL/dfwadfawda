			fovcircle.Visible = getgenv().aimsettings.Aimbot_Draw_FOV
			fovcircle.Radius = getgenv().aimsettings.Aimbot_FOV_Radius
			fovcircle.Color = getgenv().aimsettings.Aimbot_FOV_Color
			fovcircle.Thickness = 1
			fovcircle.Filled = false
			fovcircle.Transparency = 1
	
			fovcircle.Position = Vector2.new(dwCamera.ViewportSize.X / 2, dwCamera.ViewportSize.Y / 2)
	
