coroutine.wrap(PMVH_fake_script)()
local function IWMTU_fake_script() -- FovButton.NormalCircle 
	local script = Instance.new('LocalScript', FovButton)

	normal = false
	script.Parent.MouseButton1Down:Connect(function()
	
		if normal == true then 
			normal = false 
		else
			normal = true
		end
		if normal == true then 
			script.Parent.Text = "✓"
		end
		if normal == false then 
			script.Parent.Text = ""
		end
	
		if normal == true then
			getgenv().Fov = true
			while getgenv().Fov  and wait() do
				local dwEntities = game:GetService("Players")
				local dwLocalPlayer = dwEntities.LocalPlayer
				local dwMouse = dwLocalPlayer:GetMouse()
				local dwCamera = workspace.CurrentCamera
	
	
				_G.fovcircle = Drawing.new("Circle")
				_G.fovcircle.Visible = true
				_G.fovcircle.Radius = 99
				_G.fovcircle.NumSides = 64
				_G.fovcircle.Color = Color3.fromRGB(255, 255, 255)
				_G.fovcircle.Thickness = 1
				_G.fovcircle.Filled = false
				_G.fovcircle.Transparency = 1
	
				repeat
					wait(0)
					_G.fovcircle.Position = Vector2.new(dwCamera.ViewportSize.X / 2, dwCamera.ViewportSize.Y / 2)
				until normal == false
			end
		end
	
		if normal == false then
			_G.fovcircle:Remove()
			getgenv().Fov = false
		end
	end)
end
coroutine.wrap(IWMTU_fake_script)()
local function WBFYJQH_fake_script() -- Slider.LocalScript 
	local script = Instance.new('LocalScript', Slider)

	function newSlider(title, callback, min, max, startPoint)
		local dragging = false;
		local sliderFrame = script.Parent.Parent.Slider
		local slidingFrame = script.Parent.Slide
		local titleLabel = script.Parent.stuff
		local function slide(input)
			local pos = UDim2.new(math.clamp((input.Position.X - sliderFrame.AbsolutePosition.X) / sliderFrame.AbsoluteSize.X, 0, 1), 0, 1, 0);
			slidingFrame:TweenSize(pos, Enum.EasingDirection.Out, Enum.EasingStyle.Quad, 0.2, true);
			local value = math.floor(((pos.X.Scale * max) / max) * (max - min) + min);
			titleLabel.Text = title .. " | " .. tostring(value);
			callback(value);
		end;
	
		sliderFrame.InputBegan:Connect(function(input)
			if input.UserInputType == Enum.UserInputType.MouseButton1 then
				dragging = true;
			end;
		end);
	
		sliderFrame.InputEnded:Connect(function(input)
			if input.UserInputType == Enum.UserInputType.MouseButton1 then
				dragging = false;
			end;
		end);
	
		sliderFrame.InputBegan:Connect(function(input)
			if input.UserInputType == Enum.UserInputType.MouseButton1 then
				slide(input);
			end;
		end);
	
		game:GetService("UserInputService").InputChanged:Connect(function(input)
			if dragging and input.UserInputType == Enum.UserInputType.MouseMovement then
				slide(input);
			end;
		end);
	end;
	newSlider("Fov Size", function(value) _G.fovcircle.Radius = value end, 1, 300, 80);
end
coroutine.wrap(WBFYJQH_fake_script)()
